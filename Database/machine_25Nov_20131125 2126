--
-- Definition of view `latest_data_view`
--

DROP TABLE IF EXISTS `latest_data_view`;
DROP VIEW IF EXISTS `latest_data_view`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `latest_data_view` AS select `md`.`IMEI_NO` AS `imei_no`,`md`.`NAME` AS `name`,`md`.`AREA` AS `area`,`md`.`CITY` AS `city`,`md`.`STATE` AS `state`,`md`.`CUSTOMER_ID` AS `customer_id`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data1') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data1`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data2') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data2`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data3') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data3`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data4') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data4`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data5') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data5`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data6') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data6`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data7') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data7`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data8') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data8`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data9') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data9`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data10') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data10`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data11') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data11`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data12') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data12`,(select `ld1`.`VALUE` AS `data1` from `latest_data` `ld1` where ((`ld1`.`DATA_ID` = 'data13') and (`ld1`.`IMEI_NO` = `md`.`IMEI_NO`))) AS `data13` from `machine_details` `md`;



