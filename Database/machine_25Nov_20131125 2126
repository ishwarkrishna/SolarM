--
-- Definition of trigger `ADD_WRT_MACHINE_DETAILS`
--

DROP TRIGGER /*!50030 IF EXISTS */ `ADD_WRT_MACHINE_DETAILS`;

DELIMITER $$

CREATE DEFINER = `root`@`localhost` TRIGGER `ADD_WRT_MACHINE_DETAILS` AFTER INSERT ON `machine_details` FOR EACH ROW BEGIN
	INSERT INTO latest_data(data_id,data_code,imei_no)
  SELECT data_details.DATA_ID,data_code,NEW.imei_no FROM data_details,machine_details
  WHERE machine_details.imei_no=NEW.imei_no;
END $$

DELIMITER ;

